{% extends 'block.html.twig' %}
{% block content %}
<h2>Patient Resources and Support</h2>
<div class="row">
  <div class="box5 col-xs-12 col-sm-6">
    <h4>Patient Resources</h4>
    <ul class="unstyled">
        {{ content.patient_resources }}
        <li><a href="{{ path('entity.node.patient-resources', {node: content['#node'].id}) }}" class="btn">View All <i class="icon-chevron-right"></i></a></li>
    </ul>
  </div>
  <div class="col-xs-12 col-sm-6">
    <h4>Related events</h4>
  </div>
</div>
{% endblock %}